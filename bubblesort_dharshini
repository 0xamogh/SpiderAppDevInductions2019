#include <stdio.h>
 
int main()
{
  int a[100], n, i, k,c;
 
  printf("Enter number of elements");
  scanf("%d", &n);
 
  printf("Enter %d integers\n", n);
 
  for (i = 0; i < n; i++)
    scanf("%d", &array[c]);
 
  for (i = 0 ; i< n-1; i++)
  {
    for (k = 0 ; k< n-i-1; k++)
    {
      if (array[k] > array[k+1]) 
      {
        c      = a[k];
        a[k]   = a[k+1];
        a[k+1] = c;
      }
    }
  }
 
  printf("The sorted list of numbers in ascending order\n");
 
  for (i = 0; i< n; i++)
     printf("%d\n",a[i]);
 
  return 0;
}

